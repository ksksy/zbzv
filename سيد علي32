def public():
	try:
		token = open('.token.txt','r').read()
		cok = open('.cok.txt','r').read()
	except IOError:
		exit()
	try:
		jum = int(input('[?] Ø¹Ø¯Ø¯ Ø§Ù„ÙØ­Øµ  : '))
	except ValueError:
		time.sleep(3)
		print('{k}[âœ˜] Ø§Ù„Ø­Ø³Ø§Ø¨Ø§Øª Ø®Ø§ØµÙ‡  ')
		follower()
	if jum<1:
		print('[âœ˜] Ø§Ù„Ø¹Ø¯Ø¯ ÙƒØ«ÙŠØ± ')
		exit()
	ses=requests.Session()
