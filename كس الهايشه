		follower()
	if jum<1:
		print('[â˜¹ï¸] Your limit error')
		time.sleep(3)
		follower()
	ses=requests.Session()
	yz = 0
	for met in range(jum):
		yz+=1		
		kl = input('[ğŸ™ƒ] ID=> '+str(yz)+' : ')
		uid.append(kl)
	for userr in uid:
		try:
			koh2 = ses.get('https://graph.facebook.com/'+userr+'?fields=subscribers.limit(99999)&access_token='+tokenku[0],cookies={'cookie': cok}).json()
			for pi in koh2['subscribers']['data']:
				try:id.append(pi['id']+'|'+pi['name'])
				except:continue
			print('[ğŸ˜¢] Total Id : '+str(len(id)))
			setting()
		except requests.exceptions.ConnectionError:
			print('[ğŸ«¤] No Connection  ')
			exit()
		except (KeyError,IOError):
			print('[âœ˜] Id Is Not Public')
